package util;

import static org.junit.Assert.assertEquals;

import org.junit.Test;

import util.MisArrays.MinimaNota;

public class MinimaNotaTest {

    @Test
    public void testMinimaNota() {
        assertEquals(1, MinimaNota.minimaNota(new int[] { 10, 5, 1, 6, 9 }));

        assertEquals(-1, MinimaNota.minimaNota(new int[] { 1, 2, 3, 4, 5 }));
    }

}